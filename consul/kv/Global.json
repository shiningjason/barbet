{
  "DomainService": {
  },
  "Module": {
  },
  "GrpcNetworkSetting": {
    "Client": {
      "EnableCustomerSetting": true,
      "grpc.keepalive_permit_without_calls": 1,
      "grpc.http2.max_pings_without_data": 0,
      "grpc.keepalive_time_ms": 600000
    }
  },
  "AppSettings": {
    "ShutdownTimeoutInSecond": 30,
    "HttpClientTimeoutInSecond": 30
  },
  "Serilog": {
    "MinimumLevel": "Verbose",
    "Using": [
      "BarBet.Common.Logging"
    ],
    "Enrich": [
      "FromLogContext",
      "WithThreadId"
    ],
    "WriteTo:GeneralFileSubLogger": {
      "Name": "Logger",
      "Args": {
        "configureLogger": {
          "WriteTo:File": {
            "Name": "File",
            "Args": {
              "path": "../../tmp/logs/All-.log",
              "rollingInterval": "Day",
              "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{ThreadId}][{Level:u3}][{Sender}]{Message:lj}{NewLine}",
              "fileSizeLimitBytes": 5242880,
              "rollOnFileSizeLimit": true,
              "retainedFileCountLimit": 31
            }
          },
          "Filter": [
            {
              "Name": "ByExcluding",
              "Args": {
                "expression": "StartsWith(@Message, '[Large]')"
              }
            }
          ]
        }
      }
    },
    "WriteTo:LargeFileSubLogger": {
      "Name": "Logger",
      "Args": {
        "configureLogger": {
          "WriteTo:File": {
            "Name": "File",
            "Args": {
              "path": "../../tmp/logs/Large-.log",
              "rollingInterval": "Day",
              "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{ThreadId}][{Level:u3}][{Sender}]{Message:lj}{NewLine}",
              "fileSizeLimitBytes": 5242880,
              "rollOnFileSizeLimit": true,
              "retainedFileCountLimit": 31
            }
          },
          "Filter": [
            {
              "Name": "ByIncludingOnly",
              "Args": {
                "expression": "StartsWith(@Message, '[Large]')"
              }
            }
          ]
        }
      }
    }
  }
}
